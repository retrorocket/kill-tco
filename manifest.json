{
  "name": "殲滅t.co",
  "description": "ブラウザでTwitterを表示する際にt.coを展開する拡張です",
  "version": "1.20",
  "host_permissions": [
    "https://*.twitter.com/*",
    "https://*.x.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.twitter.com/*",
        "https://*.x.com/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 3
}
